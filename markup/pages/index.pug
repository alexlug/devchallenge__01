doctype html
html(class="no-js", lang="ru")

    include ../components/head/head.pug
    include ../components/header/header.pug
    include ../components/mixins/mixins.pug

    - var programList_1 = ["Chronicle of the spread of the coffee tree","What is coffee?","The structure of the coffee berry", "Arabica and Robusta", "Coffee Belt land","Fruiting coffee tree", "Seven steps of the coffee plantation to the consumer", "Breeding and hybridization of coffee tree"];
    - var programList_2 = ["Methods for collecting coffee berries", "Processing collected berries", "Calibration and sorting", "Some methods of classification and labeling of coffee beans.", "Formation of grade names.", "Storage of green (not fried) Coffee"];
    - var programList_3 = ["The technology of roasting coffee beans.", "Changes in the beans.", "Stages and types of roasting.", "Roasted coffee. Monosorta and hood.", "Decaffeination of coffee", "Flavoring", "Storage roasted coffee"];
    - var tableList = ["Coffee machine and grinder - practice - types of coffee machines and coffee grinders", "What makes professional coffee machines and coffee grinders.", "Main components of coffee machines and coffee-Lathes, autolathes. Their purpose", "How to choose a coffee machine and coffee grinder", "The structure of the machine. How to maintain, clean and customize coffee machine"];

    head
        +head(head.defaults)
    body
        section.main
            +header()
            .content__wrapper
                .main__title
                    span
                        svg.text(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" preserveAspectRatio="xMidYMid" width="89.5" height="15" viewBox="0 0 179 30")
                            image(xlink:href="data:img/png;base64,")
                    .main__title-head
                        span
                            svg.text(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" preserveAspectRatio="xMidYMid" width="101.5" height="6.5" viewBox="0 0 203 13")
                                image(xlink:href="data:img/png;base64,")
                        span.main__title-descr Intensive program barista training
                        span
                            svg.text(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" preserveAspectRatio="xMidYMid" width="101.5" height="6.5" viewBox="0 0 203 13")
                                image(xlink:href="data:img/png;base64,")
                    h1.main__h1 Barista Training in #[span amsterdam]
                    .main__title-footer On the basis of the company's largest coffee roasters
                    span
                        svg.text(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" preserveAspectRatio="xMidYMid" width="139.5" height="6.5" viewBox="0 0 279 13")
                            image(xlink:href="data:img/png;base64,")
                button.main__scroll-btn
                    span
                        svg.text(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" preserveAspectRatio="xMidYMid" width="12" height="7" viewBox="0 0 23.97 13.97")
